<!DOCTYPE html>
<html lang="en">

  <head>
    
    <!-- Meta Tag -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    
    <!-- SEO -->
    <meta name="description" content="150 words">
    <meta name="author" content="ashutosh">
    <meta name="url" content="http://www.codewithash.blog">
    <meta name="copyright" content="Code With Ash">
    <meta name="robots" content="index,follow">
    
    
    <title>CodeWithAsh's Blog</title>
    
    <!-- Favicon -->
    <link rel="shortcut icon" href="images/favicon.png">
    <link rel="apple-touch-icon" sizes="144x144" type="image/x-icon" href="images/favicon.png">
    
    <!-- All CSS Plugins -->
    <link rel="stylesheet" type="text/css" href="blog/css/plugin.css">
    
    <!-- Main CSS Stylesheet -->
    <link rel="stylesheet" type="text/css" href="blog/css/style.css">
    
    <!-- Google Web Fonts  -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:400,300,500,600,700">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

  </head>

 <body>

	
     
	 <!-- Preloader Start -->
     <div class="preloader">
	   <div class="rounder"></div>
      </div>
      <!-- Preloader End -->
      
      
    
    
    <div id="main">
        <div class="container">
            <div class="row">
            	
               
                 
                 <!-- About Me (Left Sidebar) Start -->
                 <div class="col-md-3">
                   <div class="about-fixed">
                    
                     <div class="my-pic">
                        <img src="../blogUploads/<%=blogData.image%>" alt="" style="background: transparent;">
                        <a href="javascript:void(0)" class="collapsed" data-target="#menu" data-toggle="collapse"><i class="icon-menu menu"></i></a>
                         <div id="menu" class="collapse">
                           <ul class="menu-link">
                               <li><a href="about.html">About</a></li>
                               <li><a href="work.html">Work</a></li>
                               <li><a href="contact.html">Contact</a></li>
                            </ul>
                         </div>
                        </div>
                      
                      
                      
                      <div class="my-detail">
                    	
                        <div class="white-spacing">
                            <img src="" alt="" srcset="">
                            <h1>By <%=blogData.author%></h1>
                            <span>On <%= blogData.date.toLocaleDateString() %></span>
                        </div> 
                       
                       

                    </div>
                  </div>
                </div>
                <!-- About Me (Left Sidebar) End -->
                
                
                
                
                 
                 <!-- Blog Post (Right Sidebar) Start -->
                 <div class="col-md-9">
                    <div class="col-md-12 page-body">
                    	<div class="row">
                    		
                            
                            <div class="sub-title">
                           		<h2> Code With Ash</h2>
                                <a href=""><i class="icon-envelope"></i></a>
                             </div>
    
                                <div class="col-md-12 content-page">
                                    <!-- Blog Post Start -->
                                    <div class="col-md-12 blog-post">
                                        <div class="post-title">
                                        <a href=""><h1><%= blogData.title %></h1></a>
                                        </div>  
                                        <div class="post-info">
                                            <span> <%= blogData.date.toLocaleDateString() %>  by <a href="#" target="_blank"><%= blogData.author %></a></span>
                                        </div>  
                                        
                                        <pre><%=blogData.content %></pre>                          			
                                       
                                    </div>
                    
                                   
                                    </div>
                                </div>

                            
                         </div>

                  </div>
                  <!-- Blog Post (Right Sidebar) End -->
                
            </div>
         </div>
      </div>
    
    
    
    <!-- Back to Top Start -->
    <a href="#" class="scroll-to-top"><i class="fa fa-long-arrow-up"></i></a>
    <!-- Back to Top End -->
    
    
    <!-- All Javascript Plugins  -->
    <script type="text/javascript" src="blog/js/jquery.min.js"></script>
    <script type="text/javascript" src="blog/js/plugin.js"></script>
    
    <!-- Main Javascript File  -->
    <script type="text/javascript" src="blog/js/scripts.js"></script>

    
   </body>
 </html>
